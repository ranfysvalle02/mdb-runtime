{
  "schema_version": "2.0",
  "slug": "dashboard",
  "name": "Analytics Dashboard",
  "description": "Dashboard for viewing click analytics with shared SSO authentication",
  "status": "active",
  "auth": {
    "mode": "shared",
    "related_apps": {
      "click_tracker": "http://localhost:8000"
    },
    "roles": ["clicker", "tracker", "admin"],
    "public_routes": ["/", "/health", "/api", "/login", "/logout"],
    "users": {
      "enabled": true,
      "strategy": "app_users",
      "demo_users": [
        {
          "email": "alice@example.com",
          "password": "password123",
          "app_roles": {
            "click_tracker": ["admin"],
            "dashboard": ["admin"]
          }
        },
        {
          "email": "bob@example.com",
          "password": "password123",
          "app_roles": {
            "click_tracker": ["tracker"],
            "dashboard": ["tracker"]
          }
        },
        {
          "email": "charlie@example.com",
          "password": "password123",
          "app_roles": {
            "click_tracker": ["clicker"],
            "dashboard": ["clicker"]
          }
        }
      ],
      "demo_user_seed_strategy": "auto"
    }
  },
  "cors": {
    "enabled": true,
    "allow_origins": ["http://localhost:8000", "http://localhost:8001"],
    "allow_credentials": true,
    "allow_methods": ["*"],
    "allow_headers": ["*"]
  },
  "data_access": {
    "read_scopes": ["dashboard", "click_tracker"],
    "write_scope": "dashboard"
  }
}
