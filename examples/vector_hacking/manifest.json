{
  "version": "2.0",
  "slug": "vector_hacking",
  "name": "Vector Hacking Demo",
  "description": "A demonstration of vector inversion/hacking using LLMs with MDB_ENGINE",
  "status": "active",
  "developer_id": "developer@example.com",
  "auth_required": false,
  "auth": {
    "policy": {
      "required": false,
      "allow_anonymous": true
    },
    "users": {
      "enabled": false
    }
  },
  "managed_indexes": {
    "experiments": [
      {
        "type": "regular",
        "keys": {"status": 1, "created_at": -1},
        "name": "status_created_idx",
        "options": {
          "background": true
        }
      }
    ],
    "embeddings": [
      {
        "type": "regular",
        "keys": {"source_id": 1, "chunk_index": 1},
        "name": "source_chunk_idx",
        "options": {
          "background": true
        }
      },
      {
        "type": "vectorSearch",
        "name": "embedding_vector_idx",
        "definition": {
          "fields": [
            {
              "type": "vector",
              "path": "embedding",
              "numDimensions": 1536,
              "similarity": "cosine"
            }
          ]
        }
      }
    ]
  },
  "embedding_config": {
    "enabled": true,
    "max_tokens_per_chunk": 1000,
    "default_embedding_model": "text-embedding-3-small"
  },
  "cors": {
    "enabled": true,
    "allow_origins": ["*"],
    "allow_credentials": true,
    "allow_methods": ["*"],
    "allow_headers": ["*"]
  }
}
