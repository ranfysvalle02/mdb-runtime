{
  "version": "2.0",
  "slug": "vector_hacking",
  "name": "Vector Hacking Demo",
  "description": "A demonstration of vector inversion/hacking using LLMs with MDB_RUNTIME",
  "status": "active",
  "developer_id": "developer@example.com",
  "auth_required": false,
  "auth_policy": {
    "required": false,
    "allow_anonymous": true
  },
  "sub_auth": {
    "enabled": false
  },
  "managed_indexes": {
    "experiments": [
      {
        "type": "regular",
        "keys": {"status": 1, "created_at": -1},
        "name": "status_created_idx",
        "options": {
          "background": true
        }
      }
    ],
    "voyage_embeddings": [
      {
        "type": "regular",
        "keys": {"source_id": 1, "chunk_index": 1},
        "name": "source_chunk_idx",
        "options": {
          "background": true
        }
      },
      {
        "type": "vectorSearch",
        "name": "embedding_vector_idx",
        "definition": {
          "fields": [
            {
              "type": "vector",
              "path": "embedding",
              "numDimensions": 1024,
              "similarity": "cosine"
            }
          ]
        }
      }
    ]
  },
  "websockets": {
    "realtime": {
      "path": "/ws",
      "description": "Real-time updates for vector hacking progress"
    }
  },
  "llm_config": {
    "enabled": true,
    "default_chat_model": "gpt-3.5-turbo",
    "default_embedding_model": "voyage/voyage-2",
    "default_temperature": 0.8,
    "max_retries": 4
  },
  "embedding_config": {
    "enabled": true,
    "max_tokens_per_chunk": 1000,
    "default_embedding_model": "voyage/voyage-2"
  }
}

